//problem - 2544 (LEETCODE)
//Time-Complexity - O(no. of digits).



class Solution {
    public int alternateDigitSum(int n) {
        String s = n+"";
        int sum = 0;
        for(int i = 0; i<s.length() ; i++){
            if(i%2==0){
                sum = sum + (int)s.charAt(i) - 48;
            }
            else{
                sum = sum - (int)s.charAt(i) + 48;
            }
        }
        return sum;
    }
}
