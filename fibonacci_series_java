// fibonacci series to N terms number  of terms is input by user.

import java.util.Scanner;
public class fibonacci {
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int N = input.nextInt();
        int a =0;
        int b=1; 
        int c =0;
        for(int i=0;i<N;i++){
                System.out.printf("%d ",c);
            a=b;
            b=c;
            c = a+b;
        }
        input.close();
    }
}
